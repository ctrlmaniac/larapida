generator client {
  provider = "prisma-client-js"
  output   = "prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())

  firstname String
  lastname  String

  email    String? @unique
  password String?

  isActive Boolean @default(true)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
